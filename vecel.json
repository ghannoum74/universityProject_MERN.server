{
  "version": 2,
  "builds": [{ "src": "server.js", "use": "@vercel/node" }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js",
      "methods": ["GET", "POST"],
      "headers": {
        "Access-Control-Allow-Origin": "https://main--universityprojectofficial.netlify.app/"
      }
    }
  ]
}
